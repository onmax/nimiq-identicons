{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM", "ES2023"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "types": ["vite/client", "node"],
    "strict": true,
    "strictNullChecks": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  },
  "exclude": ["playground"]
}
