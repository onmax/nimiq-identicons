{
  "name": "identicons-esm",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@10.7.1",
  "description": "A rewrite of the Identicon module with ESM",
  "license": "MIT",
  "homepage": "https://github.com/onmax/nimiq-identicons#readme",
  "repository": { "type": "git", "url": "git+https://github.com/onmax/nimiq-identicons.git" },
  "bugs": { "url": "https://github.com/onmax/nimiq-identicons/issues" },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": { "types": "./dist/index.d.mts", "import": "./dist/index.mjs" },
    "./web-component": { "types": "./dist/web-component.d.mts", "import": "./dist/web-component.mjs" },
    "./shiny": { "types": "./dist/shiny.d.mts", "import": "./dist/shiny.mjs" },
    "./shiny-web-component": { "types": "./dist/shiny-web-component.d.mts", "import": "./dist/shiny-web-component.mjs" },
    "./types": { "types": "./dist/types.d.mts", "import": "./dist/types.mjs" },
    "./core": { "types": "./dist/core.d.mts", "import": "./dist/core.mjs" }
  },
  "files": ["dist"],
  "scripts": {
    "build": "esno scripts/generate-features.ts && tsdown",
    "dev": "esno scripts/generate-features.ts && tsdown --watch",
    "optimize-svg": "esno ./scripts/optimize-features.ts"
  },
  "dependencies": {
    "@nimiq/utils": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "bumpp": "catalog:",
    "esno": "catalog:",
    "pathe": "catalog:",
    "pnpm": "catalog:",
    "svgo": "catalog:",
    "tiny-glob": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": { "access": "public" }
}
